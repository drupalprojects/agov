{
    "name": "previousnext/agov",
    "type": "drupal-profile",
    "description": "aGov is a free open source Drupal 7 distribution developed specifically for Australian government organisations",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "PreviousNext",
            "email": "info@previousnext.com.au",
            "homepage": "http://agov.com.au",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@previousnext.com.au",
        "issues": "https://www.drupal.org/project/issues/agov",
        "source": "https://github.com/previousnext/agov"
    },
    "require": {
        "composer/installers": "~1.2",
        "cweagans/composer-patches": "~1.5",
        "drupal/admin_toolbar": "1.18.0",
        "drupal/agov_base": "dev-1.x",
        "drupal/agov_whitlam": "1.2.0",
        "drupal/better_normalizers": "1.0.0-beta1",
        "drupal/core": "8.2.3",
        "drupal/ctools": "3.0.0-alpha27",
        "drupal/default_content": "1.0.0-alpha3",
        "drupal/ds": "2.6.0",
        "drupal/embed": "1.0.0-rc3",
        "drupal/entity": "1.0.0-alpha3",
        "drupal/entity_browser": "1.0.0-beta2",
        "drupal/entity_embed": "1.0.0-beta2",
        "drupal/fences": "2.0.0-alpha1",
        "drupal/inline_entity_form": "1.0.0-beta1",
        "drupal/layout_plugin": "1.0.0-alpha23",
        "drupal/link_attributes": "1.0.0-alpha3",
        "drupal/media_entity": "1.6.0",
        "drupal/media_entity_browser": "1.0.0-beta2",
        "drupal/media_entity_image": "1.2.0",
        "drupal/metatag": "1.0.0-beta11",
        "drupal/page_manager": "1.0.0-alpha24",
        "drupal/panels": "3.0.0-beta5",
        "drupal/password_policy": "dev-3.x#896639711c174e45e62667613b163a571e64d926",
        "drupal/pathauto": "1.0.0-beta1",
        "drupal/scheduled_updates": "1.0.0-alpha5",
        "drupal/simple_sitemap": "2.7.0",
        "drupal/token": "1.0.0-beta2",
        "drupal/twitter_block": "2.1.0",
        "drupal/video_embed_field": "1.3.0",
        "drupal/workbench_moderation": "1.1.0"
    },
    "require-dev": {
        "drupal-composer/drupal-scaffold": "~2.2",
        "drupal/coder": "~8.2",
        "drupal/config_devel": "~1.0",
        "drupal/console": "~0.11",
        "drush/drush": "~8.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "extra": {
        "installer-paths": {
            "app/core": [
                "drupal/core"
            ],
            "app/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "app/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "app/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/page_manager": {
                "Fix contextual links on page manager pages": "https://www.drupal.org/files/issues/page_manager-block_content_contextual-2601004-14.patch"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin/"
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ]
    }
}
